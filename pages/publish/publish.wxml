<!--pages/publish/publish.wxml-->
<van-cell-group>
  <van-field
    value="{{place}}"
    required
    clearable
    label="任务地点"
    icon="question-o"
    placeholder="请输入任务地点"
    bind:click-icon="onClickIcon"
  />
  <van-field
    value="{{name}}"
    required
    clearable
    label="任务名称"
    icon="question-o"
    placeholder="请输入任务名称"
    bind:click-icon="onClickIcon"
  /><van-field
    value="{{place}}"
    required
    clearable
    label="任务地点"
    icon="question-o"
    placeholder="请输入任务地点"
    bind:click-icon="onClickIcon"
  /><van-field
    value="{{requirement}}"
    required
    clearable
    label="任务要求"
    icon="question-o"
    placeholder="请输入任务要求"
    bind:click-icon="onClickIcon"
  /><van-field
    value="{{money}}"
    required
    clearable
    label="金额"
    icon="question-o"
    placeholder="请输入金额"
    bind:click-icon="onClickIcon"
  /><van-field
    value="{{DDL}}"
    required
    clearable
    label="要求时间"
    icon="question-o"
    placeholder="请输入要求时间"
    bind:click-icon="onClickIcon"
  /><van-field
    value="{{userName}}"
    required
    clearable
    label="发布者昵称"
    icon="question-o"
    placeholder="请输入发布者昵称"
    bind:click-icon="onClickIcon"
  /><van-field
    value="{{phone}}"
    required
    clearable
    label="发布者联系方式"
    icon="question-o"
    placeholder="请输入发布者联系方式"
    bind:click-icon="onClickIcon"
  />
  <van-button round color="green" size="large">发布</van-button>
</van-cell-group>